{
  "name": "urdf-loader",
  "version": "0.8.2",
  "description": "URDF Loader for THREE.js and webcomponent viewer",
  "main": "umd/URDFLoader.js",
  "module": "src/URDFLoader.js",
  "scripts": {
    "start": "concurrently --kill-others \"rollup -c -w\" \"cd .. && static-server\"",
    "build": "rollup -c",
    "test": "jest",
    "lint": "eslint ./src/*.js ./test/*.js && tsc -p tsconfig.json --noEmit",
    "coverage": "nyc report --reporter=html & opn coverage/index.html"
  },
  "files": [
    "src/*",
    "umd/*",
    "example/index.js",
    "example/dragAndDrop.js",
    "example/styles.css"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gkjohnson/urdf-loaders.git"
  },
  "bugs": {
    "url": "https://github.com/gkjohnson/urdf-loaders/issues"
  },
  "homepage": "https://github.com/gkjohnson/urdf-loaders#readme",
  "keywords": [
    "javascript",
    "threejs",
    "graphics",
    "ros",
    "robotics",
    "urdf",
    "urdf-models",
    "webcomponents"
  ],
  "peerDependencies": {
    "three": ">=0.105.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "babel-jest": "^25.4.0",
    "concurrently": "^4.1.2",
    "eslint": "^7.10.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^25.4.0",
    "jest-cli": "^25.4.0",
    "jsdom": "^16.4.0",
    "nyc": "^13.3.0",
    "opn-cli": "^3.1.0",
    "puppeteer": "^1.20.0",
    "puppeteer-to-istanbul": "^1.4.0",
    "rollup": "^2.29.0",
    "static-server": "^3.0.0",
    "three": "^0.119.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {}
}
